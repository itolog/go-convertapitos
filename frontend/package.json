{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "gen-api-types": "npx swagger-typescript-api generate -p http://localhost:3000/api/v1/swagger/doc.json -o ./src/generated/apiClient --modular",
    "check-deps": "ncu",
    "up-deps": "ncu -u"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/vue-query": "^5.77.2",
    "@tanstack/vue-query-devtools": "^5.77.2",
    "@tanstack/vue-table": "^8.21.3",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^13.2.0",
    "@vueuse/integrations": "^13.2.0",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.511.0",
    "pinia": "^3.0.2",
    "reka-ui": "^2.2.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "tw-animate-css": "^1.3.0",
    "universal-cookie": "^7.2.2",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.15",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@iconify-json/radix-icons": "^1.2.2",
    "@iconify/vue": "^5.0.0",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.27.0",
    "eslint-plugin-oxlint": "^0.16.12",
    "eslint-plugin-vue": "~10.1.0",
    "jiti": "^2.4.2",
    "npm-check-updates": "^18.0.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^0.16.12",
    "prettier": "3.5.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-tsc": "^2.2.10"
  }
}
